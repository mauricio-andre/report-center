{
  "ConnectionStrings": {
    "CoreDbContext": ""
  },
  "EnvironmentName": "DEV",
  "ServiceName": "ReportCenterGrpcServer",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "JsonCustom",
      "FormatterOptions": {
        "IncludeScopes": false,
        "IncludeDetails": false,
        "EnvironmentName": "DEV",
        "ServiceName": "ReportCenterGrpcServer",
        "JsonWriterOptions": {
          "Indented": false
        }
      }
    }
  },
  "OpenTelemetry": {
    "Logging": {
      "Enable": false,
      "ConsoleExporter": false,
      "OtlpExporter": {
        "Endpoint": ""
      }
    },
    "Tracing": {
      "Enable": false,
      "ConsoleExporter": false,
      "OtlpExporter": {
        "Endpoint": ""
      }
    },
    "Metrics": {
      "Enable": false,
      "ConsoleExporter": false,
      "OtlpExporter": {
        "Endpoint": ""
      }
    }
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
